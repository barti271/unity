<configuration>
  <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%date [%10.10thread] [%8.-8X{user}] [%10.10X{tenant}] [%8.-8X{plan}] %-5level %30.30logger - %msg%n</pattern>
    </encoder>
  </appender>
  
  <root level="DEBUG">
    <appender-ref ref="console" />
  </root>  
</configuration>
